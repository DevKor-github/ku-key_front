{
  "trailingComma": "all",
  "tabWidth": 2,
  "printWidth": 120,
  "singleQuote": true,
  "arrowParens": "avoid",
  "semi": false,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "proseWrap": "never",
  "endOfLine": "auto",
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
